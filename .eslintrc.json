{
	"extends": [
		"eslint:recommended",
		"prettier"
	],
	"plugins": [
		"prettier"
	],
	"env": {
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2017
	},
	"rules": {
		"prettier/prettier": ["error",{
			"endOfLine": "lf"}
		],
			
		"no-unused-vars": [
			"error",
			{
				"argsIgnorePattern": "^_"
			}
		],
		"no-console": "off",
		"semi": [
			"error",
			"always"
		],
		"linebreak-style": 0
	}
}